		<object-name>intvitres</object-name>
		<object-name>intvitreporte</object-name>
	</animation>

	<!-- working for Rembrandt -->
	<effect>
		<inherits-from>Aircraft/TwinBeech-Jsb/Models/Effects/Glass/glass</inherits-from>
		<object-name>intvitres</object-name>
		<object-name>intvitreporte</object-name>
	</effect>
  
	<!-- Pilote -->
	<model>
		<path>Aircraft/TwinBeech-Jsb/Models/Pilot/pilot.xml</path>
		<offsets>
			<x-m> -3.07184 </x-m>
			<y-m> -0.32198 </y-m>
			<z-m> -0.12676 </z-m>
		</offsets>
	</model>

	<!-- Tableau de bord -->
	<model>
		<path>Aircraft/TwinBeech-Jsb/Models/Interior/Panel/panel.xml</path>
		<offsets>
			<x-m> -3.55960 </x-m>
			<y-m>  0.00000 </y-m>
			<z-m>  0.18792 </z-m>
		</offsets>
	</model>

	<!-- Les siÃ¨ges -->
	<model>
		<path>Aircraft/TwinBeech-Jsb/Models/Interior/Seats/all-seats.xml</path>
	</model>

	<!-- Animation des portes -->
	<animation>
		<name>IntLaPorte</name>
		<object-name>intporte</object-name>
		<object-name>intvitreporte</object-name>
	</animation>

	<animation>
		<type>pick</type>
		<object-name>IntLaPorte</object-name>
		<visible>true</visible>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>nasal</command>
				<script>doors.crew.toggle();</script>
			</binding>
		</action>
	</animation>
  
	<animation>
		<type>rotate</type>
		<object-name>IntLaPorte</object-name>
		<property>sim/model/door-positions/crew/position-norm</property>
		<factor> -170 </factor>
		<center>
			<x-m> -0.00953 </x-m>
			<y-m> -0.72224 </y-m>
			<z-m>  0.00796 </z-m>
		</center>
		<axis>
			<x> 0 </x>
			<y> 0 </y>
			<z> 1 </z>
		</axis>
	</animation>

	<!-- Fermeture des portes des trains -->
	<animation>
